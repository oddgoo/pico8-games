function update_columns()

 if btn(‚ùé) or btn(‚¨ÜÔ∏è) then
  if (cs[1].s==0) sfx(2)
  cs[1].s = 1
  if (scene=="game") cs[1].l-=0.05
 else
  if cs[1].s==1 then
  	cs[1].s = 0
  	--offset = .1
    if check_squash(cs[1].x, -1) then
      sfx(0)
    else 
      sfx(1)
    end
  end
  if(cs[1].l<max_l) cs[1].l+=0.02
 end

 if (cs[1].l<= 0) and scene=="game" then
  death_message = "empty!"
  lose()
 end
--  if(cs[1].l>max_l) cs[1].l=max_l

 if btn(üÖæÔ∏è) or btn(‚¨áÔ∏è) then
  if (cs[2].s==0) sfx(2)
 	cs[2].s = 1
 	if (scene=="game") cs[2].l-=0.05
 else
  if cs[2].s==1 then
  	cs[2].s = 0
  	offset = .1
    if check_squash(cs[2].x, 1) then
      sfx(0)
    else 
      sfx(1)
    end
  end
  if(cs[2].l<max_l) cs[2].l+=0.02
 end

 if (cs[2].l<= 0) and scene=="game" then
  death_message = "empty!"
  lose()
 end
--  if(cs[2].l>max_l) cs[2].l=max_l
 
end

function check_squash(x,h)
  for i=1,#beats do
    b = beats[i]
    if (b.h == h and b.x > x-5 and b.x < x+5  ) then
       b.h = -b.h
       b.x += b.d*2
    --   return true
    end
  end
  return false
end

function draw_columns()
 draw_c‚¨ÜÔ∏è(cs[1].x -2,
 cs[1].s,cs[1].l)
 draw_c‚¨áÔ∏è(cs[2].x -2,
 cs[2].s,cs[2].l)

end

function draw_c‚¨ÜÔ∏è(x,s,l)
 local y = 32
 rectfill(x,
 y-s*10-l*8,
 x+4,
 y-s*10,cl1)
end

function draw_c‚¨áÔ∏è(x,s,l)
 local y = 31
 rectfill(x,
 y+s*10+l*8,
 x+4,
 y+s*10,cl2)
end

